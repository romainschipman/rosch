{
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)node_modules|dist"
    },
    "opts": {
      "destination": "./docs",
      "recurse": true,
      "verbose": true
    },
    "plugins": [
      "plugins/markdown"
    ],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "typescript": {
      "enabled": true,
      "moduleRoot": "src"
    }
  }
  